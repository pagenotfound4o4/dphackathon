<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration SYSTEM 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory>
 <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
 <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
 <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/takeme?useUnicode=true&amp;characterEncoding=utf-8</property>
 <property name="hibernate.connection.username">takeme</property>
 <property name="hibernate.connection.password">takeme</property>
 <property name="show_sql">true</property>
 <mapping class="main.model.User"></mapping>
 <mapping class="main.model.Favorite"/>
 <mapping class="main.model.Tmp"/>
 <mapping class="main.model.Timeline"/>
</session-factory>
</hibernate-configuration>